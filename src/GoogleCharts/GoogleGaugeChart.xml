<?xml version="1.0" encoding="utf-8" ?>
<widget id="GoogleCharts.widget.GoogleGaugeChart" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>Google Gauge Chart</name>
    <description>A gauge with a dial, rendered within the browser using SVG or VML. </description>

    <icon></icon>

    <properties>
        <property key="animationDuration" type="attribute" required="false">
            <caption>Duration</caption>
            <category>Animation</category>
            <description>The duration of the animation, in milliseconds. (Default: 400)</description>
            <attributeTypes>
                <attributeType name="Integer"/>
            </attributeTypes>
        </property>
        <property key="animationEasing" type="enumeration" required="true" defaultValue="linear">
            <caption>Easing</caption>
            <category>Animation</category>
            <description>The easing function applied to the animation.</description>
            <enumerationValues>
                <enumerationValue key="linear">linear</enumerationValue>
                <enumerationValue key="in">in</enumerationValue>
                <enumerationValue key="out">out</enumerationValue>
                <enumerationValue key="inAndOut">inAndOut</enumerationValue>
            </enumerationValues>
        </property>
        <property key="forceIFrame" type="boolean" required="true" defaultValue="false">
            <caption>Force Inline Frame</caption>
            <category>Behavior</category>
            <description>Draws the chart inside an inline frame. (Note that on IE8, this option is ignored; all IE8 charts are drawn in i-frames.)</description>
        </property>
        <property key="greenColor" type="attribute" required="false">
            <caption>Green Color</caption>
            <category>Appearance</category>
            <description>The color to use for the green section, in HTML color notation. (Default: '#109618')</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="greenFrom" type="attribute" required="false">
            <caption>Green From</caption>
            <category>Appearance</category>
            <description>The lowest value for a range marked by a green color.</description>
            <attributeTypes>
                <attributeType name="Integer"/>
            </attributeTypes>
        </property>
        <property key="greenTo" type="attribute" required="false">
            <caption>Green To</caption>
            <category>Appearance</category>
            <description>The highest value for a range marked by a green color.</description>
            <attributeTypes>
                <attributeType name="Integer"/>
            </attributeTypes>
        </property>
        <property key="height" type="integer" defaultValue="0">
            <caption>Height</caption>
            <category>Appearance</category>
            <description>Height of the chart in pixels. (Default: container's width)</description>
        </property>
        <property key="width" type="integer" defaultValue="0">
            <caption>Width</caption>
            <category>Appearance</category>
            <description>Width of the chart in pixels. (Default: container's width)</description>
        </property>
        <property key="max" type="attribute" required="false">
            <caption>Max</caption>
            <category>Appearance</category>
            <description>The maximal value of a gauge. (Default: 100)</description>
            <attributeTypes>
                <attributeType name="Integer"/>
            </attributeTypes>
        </property>
        <property key="min" type="attribute" required="false">
            <caption>Min</caption>
            <category>Appearance</category>
            <description>The minimal value of a gauge. (Default: 0)</description>
            <attributeTypes>
                <attributeType name="Integer"/>
            </attributeTypes>
        </property>
        <property key="redColor" type="attribute" required="false">
            <caption>Red Color</caption>
            <category>Appearance</category>
            <description>The color to use for the red section, in HTML color notation. (Default: '#DC3912')</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="redFrom" type="attribute" required="false">
            <caption>Red From</caption>
            <category>Appearance</category>
            <description>The lowest value for a range marked by a red color.</description>
            <attributeTypes>
                <attributeType name="Integer"/>
            </attributeTypes>
        </property>
        <property key="redTo" type="attribute" required="false">
            <caption>Red To</caption>
            <category>Appearance</category>
            <description>The highest value for a range marked by a red color.</description>
            <attributeTypes>
                <attributeType name="Integer"/>
            </attributeTypes>
        </property>
        <property key="yellowColor" type="attribute" required="false">
            <caption>Yellow Color</caption>
            <category>Appearance</category>
            <description>The color to use for the yellow section, in HTML color notation. (Default: '#FF9900')</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="yellowFrom" type="attribute" required="false">
            <caption>Yellow From</caption>
            <category>Appearance</category>
            <description>The lowest value for a range marked by a yellow color.</description>
            <attributeTypes>
                <attributeType name="Integer"/>
            </attributeTypes>
        </property>
        <property key="yellowTo" type="attribute" required="false">
            <caption>Yellow To</caption>
            <category>Appearance</category>
            <description>The highest value for a range marked by a yellow color.</description>
            <attributeTypes>
                <attributeType name="Integer"/>
            </attributeTypes>
        </property>
        <property key="jsonDataSource" type="attribute" required="true">
            <caption>JSON</caption>
            <category>Data Source</category>
            <description>The data source of the gauge.</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="mfToExecute" type="microflow" required="false" defaultValue="">
            <caption>Microflow</caption>
            <category>Behavior</category>
            <description>The microflow to execute on click.</description>
            <returnType type="Boolean"></returnType>
        </property>
    </properties>
</widget>
